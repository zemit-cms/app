{
  "name": "zemit-cms/app",
  "version": "1.0.0",
  "type": "project",
  "description": "Start a new Zemit Project",
  "keywords": ["framework", "phalcon", "cms", "zemit", "zemit-cms", "zemit cms", "zemit cms"],
  "homepage": "https://www.zemit.com",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Zemit Team",
      "email": "contact@zemit.com",
      "homepage": "https://www.zemit.com/"
    },
    {
      "name": "Nuagerie Team",
      "email": "contact@nuagerie.com",
      "homepage": "https://www.nuagerie.com/"
    },
    {
      "name": "Julien Turbide",
      "email": "contact@jturbide.com",
      "homepage": "https://www.jturbide.com/"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/zemit-cms/core/graphs/contributors"
    }
  ],
  "support": {
    "issues": "https://github.com/zemit-cms/core/issues",
    "source": "https://github.com/zemit-cms/core"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "optimize-autoloader": true,
  "apcu-autoloader": true,
  "apcu-autoloader-prefix": "zemit-app-",
  "require": {
    "php": ">=8.4",
    "ext-phalcon": ">=5.9.3",
    "zemit-cms/core": "^1.0 || dev-master"
  },
  "require-dev": {
    "phalcon/ide-stubs": "^5.9.3",
    "phpspec/phpspec": "^7.5",
    "squizlabs/php_codesniffer": "^3.8",
    "phpunit/phpunit": "^10.5"
  },
  "extra": {
    "installer-paths": {
      "public/vendor/{$name}": [
        "zemit-cms/workspace"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\": "tests/Unit",
      "Zemit\\Tests\\Unit\\": "vendor/zemit-cms/core/tests"
    }
  },
  "bin":[]
}
